{
  "name": "random-from-collection",
  "version": "1.0.2",
  "description": "Get a random set of n distinct values from a standard collection (i.e. Map, Set, Array)",
  "main": "index.js",
  "scripts": {
    "test": "nyc -x fixtures/ -x test.js mocha test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:debug": "mocha --inspect-brk test.js",
    "lint": "eslint --fix *.js fixtures/*.js",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/decompil3d/random-from-collection.git"
  },
  "keywords": [
    "random",
    "collection",
    "set",
    "map",
    "rand",
    "distinct",
    "unique"
  ],
  "author": "Jonathan Keslin <jonathan@jonathankeslin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/decompil3d/random-from-collection/issues"
  },
  "homepage": "https://github.com/decompil3d/random-from-collection#readme",
  "devDependencies": {
    "assume": "^2.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^6.0.0",
    "eslint-config-godaddy": "^4.0.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-mocha": "^6.0.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0"
  }
}
